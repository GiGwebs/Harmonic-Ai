    "test": "FIRESTORE_EMULATOR_HOST=localhost:8080 jest",
    "test:watch": "FIRESTORE_EMULATOR_HOST=localhost:8080 jest --watch",
    "emulator": "firebase emulators:start --only firestore",
    "test:original": "jest --config jest.config.cjs"
